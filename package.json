{
  "name": "@soerenmartius/vue3-clipboard",
  "version": "0.0.4",
  "description": "A Vue 3 binding for clipboard.js",
  "main": "vue-clipboard.js",
  "scripts": {
    "build": "$(npm bin)/eslint vue-clipboard.js && $(npm bin)/browserify browserify-me.js -o dist/vue-clipboard.js && $(npm bin)/minify dist/vue-clipboard.js -o dist/vue-clipboard.min.js && echo 'Build Successful'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soerenmartius/vue3-clipboard.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "clipboard",
    "clipboard.js"
  ],
  "author": "soerenmartius",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soerenmartius/vue3-clipboard/issues"
  },
  "homepage": "https://github.com/soerenmartius/vue3-clipboard#readme",
  "peerDependecies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@types/clipboard": "^2.0.1",
    "clipboard": "^2.0.0"
  },
  "devDependencies": {
    "babel-minify": "^0.5.1",
    "babel-preset-minify": "^0.5.1",
    "browserify": "^16.5.0",
    "eslint": ">=7.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": ">=2.22.0",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-promise": ">=4.2.0",
    "eslint-plugin-standard": ">=4.0.0"
  }
}
